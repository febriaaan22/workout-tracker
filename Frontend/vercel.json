{
  "version": 2,
  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/"
      }
  ],
  "headers": [
      {
          "source": "/(.*)",
          "headers": [
              {
                  "key": "X-Content-Type-Options",
                  "value": "nosniff"
              },
              {
                  "key": "X-Frame-Options",
                  "value": "DENY"
              },
              {
                  "key": "Referrer-Policy",
                  "value": "no-referrer"
              },
              {
                  "key": "X-DNS-Prefetch-Control",
                  "value": "on"
              },
              {
                  "key": "Strict-Transport-Security",
                  "value": "max-age=15552000; includeSubdomains"
              },
              {
                  "key": "Content-Security-Policy",
                  "value": "frame-ancestors 'none'"
              }
          ]
      }
  ]
}